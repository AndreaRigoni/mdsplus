#!/bin/bash
if [ -z $VALGRIND_TOOL ]
then JVM_OPTIONS="-XX:UseSSE=0 -Djava.compiler=NONE"
elif [ "$VALGRIND_TOOL" = "helgrind" ]
then exit 77
else JVM_OPTIONS=
fi
echo "JUNITTEST=${JUNITTEST}"
exec ${JAVA_PATH_NAME} ${JAVA_TARGET_ARCH} $JVM_OPTIONS ${JUNITTEST:-org.junit.runner.JUnitCore} ${2/"$1/"/}
